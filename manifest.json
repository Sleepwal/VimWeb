{
  "manifest_version": 3,
  "name": "Vim Web",
  "version": "0.1",
  "description": "Make the web navigable by keyboard, Vim-style.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
